#include<stdio.h>

int main()
{
    int ar[8][8];
    char temp;
    int ides, jdes;
    for (int i = 0; i<8; i++)
    {
        for (int j =0; j<8; j++)
        {
            scanf("%c",&temp);
            if(temp=='.') ar[i][j]=0;
            else if(temp=='2') {ides=i; jdes=j; ar[i][j]=2;}
            else if(temp=='P') ar[i][j]=1;
            else if(temp=='B') ar[i][j]=3;
            else if(temp=='K') ar[i][j]=5;
            else if(temp=='R') ar[i][j]=3;
            else if(temp=='Q') ar[i][j]=8;
            else if(temp=='A') ar[i][j]=10;
            else ar[i][j]=0;
            
            
        }
    }
    printf("%d %d",ides, jdes);
     for (int i = 0; i < 8; i++)
    {
        for (int j = 0; j<8; j++)
	  {printf("%d ", ar[i][j]);

	  }
	printf("\n");
    }


    
    
}
